{
  "name": "blog",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint . && prettier --write '**/*.js' --loglevel warn",
    "dev": "next -p 3000",
    "build": "yarn rss && next build",
    "rss": "node lib/rss"
  },
  "pre-commit": "lint",
  "license": "MIT",
  "dependencies": {
    "next": "^11.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rss-parser": "^3.12.0",
    "swr": "^0.5.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.20.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gray-matter": "^4.0.2",
    "marked": "^2.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-hover-media-feature": "^1.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "prism-react-renderer": "^1.1.1",
    "rss": "^1.2.2",
    "typescript": "^4.1.5"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "next-unused": {
    "alias": {
      "@components": "components/",
      "@data": "data/",
      "@lib": "lib/",
      "@styles": "styles/"
    },
    "include": [
      "components",
      "lib"
    ]
  }
}
