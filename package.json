    "@next/mdx": "13.2.5-canary.24",
    "@supabase/supabase-js": "1.35.6",
    "@vercel/analytics": "^0.1.11",
    "@vercel/og": "^0.0.27",
    "bright": "^0.7.0",
    "globby": "^11.0.0",
    "lodash.debounce": "4.0.8",
    "next": "13.2.5-canary.24",
    "next-mdx-remote": "^4.4.1",
    "next-remote-watch": "2.0.0",
    "next-themes": "^0.2.1",
    "postcss": "8.4.21",
