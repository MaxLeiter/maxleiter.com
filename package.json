    "@types/marked": "4.0.7",
    "@types/mdx": "^2.0.3",
    "@types/node": "18.7.16",
    "@types/prettier": "2.7.0",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/rss": "0.0.29",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "clsx": "^1.2.1",
    "concurrently": "7.4.0",
